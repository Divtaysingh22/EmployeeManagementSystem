<h1>Edit Post</h1>
<%=form_for @contects do |f|%>

	<label>Name</label>
	<%=f.text_field :name%>
	<br>
	<label>Email</label>
	<%=f.text_area :email%>
	<br>
	<label>Mobile</label>
	<%=f.text_area :mobile%>
	<br>
	<label>City</label>
	<%=f.text_area :city%>
	<br>
	<label>State</label>
	<%=f.text_area :state%>
	<br>
	
	<%=f.submit "Update Post"%>
<% end -%>